						<folder name="test">
							<folder name="Source_Code">
								<folder name="test" marked="1" recursemasks="1"/>
							</folder>
						</folder>
						<folder name="test" unmarked="1"/>
					</folder>
					<folder name="test" unmarked="1">
						<folder name="test">
							<folder name="test" marked="1" recursemasks="1"/>
